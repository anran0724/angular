<div id="HostDashboard">
    <div class="m-subheader">
        <div class="d-flex align-items-center">
            <div class="mr-auto">
                <h3 class="m-subheader__title m-subheader__title--separator">
                    <span>@L("Dashboard")</span>
                </h3>
                <span class="m-section__sub d-none d-sm-inline-block">
                    @L("HostDashboardHeaderInfo")
                </span>
            </div>
            <div>
                <button name="RefreshButton" class="btn btn-primary">
                    <i class="la la-refresh"></i>
                    <span class="d-none d-sm-inline-block">
                        @L("Refresh")
                    </span>
                </button>              
            </div>
        </div>
    </div>

    <div class="m-content">
        <div class="m-portlet">
            <div class="m-portlet__body  m-portlet__body--no-padding">
                <div class="row m-row--no-padding m-row--col-separator-xl">
                    <div class="col-md-12 col-lg-6 col-xl-4">
                        <div class="m-widget24 new-subscription-statistics">
                            <div class="m-widget24__item">
                                <h4 class="m-widget24__title">
                                    @L("ElevatorCountZ")
                                </h4>
                                <br>
                                <span class="m-widget24__desc">
                                    @L("Elevator")
                                </span>
                                <span class="m-widget24__stats m--font-brand">
                                    <span class="new-subscription-amount counterup elevatorCountZ">...</span>
                                </span>
                                <div class="m--space-10"></div>
                               
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-6 col-xl-4">
                        <div class="m-widget24 new-tenants-statistics">
                            <div class="m-widget24__item">
                                <h4 class="m-widget24__title">
                                    @L("ElevatorCount")
                                </h4>
                                <br>
                                <span class="m-widget24__desc">
                                    @L("Elevator")
                                </span>
                                <span class="m-widget24__stats m--font-info new-tenants-count  elevatorCount counterup">
                                    ...
                                </span>
                                <div class="m--space-10"></div>
                              
                            </div>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-6 col-xl-4">
                        <div class="m-widget24 dashboard-statistics1">
                            <div class="m-widget24__item">
                                <h4 class="m-widget24__title">
                                    @L("ExceedMaintenanceCount")
                                </h4>
                                <br>
                                <span class="m-widget24__desc">
                                    @L("Maintenance")
                                </span>
                                <span class="m-widget24__stats m--font-warning   exceedMaintenanceCount counterup">
                                    ...
                                </span>
                                <div class="m--space-10"></div>
                               
                            </div>
                        </div>
                    </div>

                </div>

            </div>
        </div>

        <div class="row">
            <div class="m-portlet m-portlet--full-height expiring-tenants col-xl-12">
                <div class="row m-row--no-padding m-row--col-separator-xl">
                    <div class="col-md-12 col-lg-6 col-xl-6">
                        <div class="m-widget24 new-subscription-statistics">
                            <div class="m-widget24__item">
                                <h4 class="m-widget24__title">
                                    @L("FaultElevatorCount")
                                </h4>
                                <br>
                                <span class="m-widget24__desc">
