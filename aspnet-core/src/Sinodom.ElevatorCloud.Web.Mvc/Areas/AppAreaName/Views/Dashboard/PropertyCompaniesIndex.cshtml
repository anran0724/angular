                        </div>
                    </div>*@
                </div>
            </div>
        </div>

        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="m-portlet m-portlet--full-height expiring-tenants">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <h3 class="m-portlet__head-text">
                                    <i class="icon-graduation font-red-pink"></i>
                                    <span class="caption-subject font-red-pink bold uppercase">@L("ECCPBaseMaintenanceCompanies")</span><br />
                                    <small>
                                        <span class="caption-helper"></span>
                                    </small>
                                </h3>
                            </div>
                        </div>
                    </div>
                    <div class="m-portlet__body">
                        <div class="scroller chart m-scrollable" data-always-visible="1" data-rail-visible="0" data-scrollable="true">
                            <table class="expiring-tenants-table display table table-striped table-bordered table-hover dt-responsive nowrap">
                                <thead>
                                    <tr>
                                        <th>@L("ECCPBaseMaintenanceCompanyName")</th>
                                        <th>@L("ElevatorCount")</th>
                                        <th>@L("PeriodElevatorCount")</th>
                                        <th>@L("ExceedMaintenanceCount")</th>
                                        <th>@L("CompleteMaintenanceCount")</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                        <div class="scroller-footer">
                            <div class="btn-arrow-link pull-right">
                            </div>
                        </div>
                    </div>
                </div>
            </div>
